        "def fact(N):\n",
        "\n",
        "  if N==0:\n",
        "    n = 1\n",
        "  elif (N<0 or (int(N)!=N)):\n",
        "    n = \"Error, check number\"\n",
        "  else:\n",
        "    n = N\n",
        "    for k in range (1, N-1):\n",
        "      n=n*(N-k)\n",
        "  return n\n",
        "\n",
        "print(fact(2.5))"
      ]
